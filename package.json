{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "description": "React template with Webpack and Babel",
  "main": "app",
  "scripts": {
    "prepare": "husky install",
    "start": "webpack serve --hot --config webpack/webpack.dev.js",
    "start:production": "webpack serve --hot --config webpack/webpack.prod.js",
    "test": "jest",
    "format": "prettier --write \"app/**/*.js\" \"webpack/**/*.js\"",
    "lint": "eslint \"{app,webpack}/**/*.js\" --fix",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint",
    "build:clean": "rimraf ./build",
    "prebuild": "npm run build:clean",
    "build": "webpack --color --progress --config webpack/webpack.prod.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@headlessui/react": "^1.7.16",
    "@mui/material": "^5.12.1",
    "@mui/x-data-grid": "^6.2.1",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.6",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.2",
    "dot-object": "^2.1.4",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "phone": "^3.1.37",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-date-range": "^1.4.0",
    "react-datepicker": "^4.12.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-pin-input": "^1.3.1",
    "react-rating": "^2.0.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.2.1",
    "react-select": "^5.7.3",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
    "redux-saga": "^1.2.2",
    "swiper": "^9.4.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv": "^14.3.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "{app,webpack}/**/*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
